{
  "extends": "airbnb/base",
  "rules": {
    "semi": 0,
    "object-shorthand": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "strict":[2, "global"],
    "func-names": 0,
    "space-before-function-paren": 0,
    "arrow-body-style": 0,
    "prefer-template": 0
  },
  "env":{
    "node":true,
    "es6":true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script"
  }
}
